{
    "parser": "babel-eslint",
    "rules": {
        "max-len": [1, 80, 4, {"ignoreComments": true}],
        "no-underscore-dangle": 0,
        "func-names": ["error", "never"],
        "no-bitwise": 0
    },
    "extends": ["airbnb-base"],
    "globals": {
      "pc": true,
      "window": true
    }
}